{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "yarn lint:commit && yarn lint:eslint && yarn lint:prettier",
    "lint:fix": "yarn lint:eslint:fix && yarn lint:prettier:fix",
    "lint:commit": "COMMIT=$(git log master.. --format=%P | tail -1) && COMMIT=${COMMIT:-$(git log -1 --format=%P)} && commitlint --from=$COMMIT",
    "lint:eslint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:prettier": "prettier --check \"**/*.{js,jsx,json,ts,tsx,yml}\"",
    "lint:prettier:fix": "yarn lint:prettier --write"
  },
  "dependencies": {
    "@expo-google-fonts/indie-flower": "^0.1.0",
    "@expo-google-fonts/work-sans": "^0.1.0",
    "@react-native-async-storage/async-storage": "^1.13.2",
    "expo": "~40.0.0",
    "expo-app-loading": "^1.0.1",
    "expo-font": "^8.4.0",
    "expo-linear-gradient": "^8.4.0",
    "expo-status-bar": "~1.0.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-calendars": "^1.1129.0",
    "react-native-svg": "^12.1.0",
    "react-native-web": "~0.13.12",
    "toggle-switch-react-native": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "@types/react-native-calendars": "^1.505.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "typescript": "~4.0.0"
  },
  "private": true
}
